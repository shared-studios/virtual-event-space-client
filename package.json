{
  "name": "virtual-event-space-client",
  "version": "0.1.0",
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm run delete-build && react-scripts build && npm run empty-s3-bucket && npm run upload-to-s3",
    "upload-to-s3": "aws s3 cp build s3://virtual-event-space-client/ --recursive",
    "create-s3-bucket": "aws s3 mb s3://virtual-event-space-client",
    "empty-s3-bucket": "aws s3 rm s3://virtual-event-space-client --recursive",
    "delete-build": "rm -rf build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-logger": "^3.0.6"
  }
}